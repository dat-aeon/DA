			</rule>
			<rule if-outcome="register">
				<redirect view-id="/view/DPT002.xhtml" />
			</rule>
			<rule if-outcome="updateInit">
				<redirect view-id="/view/DPT002.xhtml" />
			</rule>
		</navigation>
	</page>
	
	<page view-id="/view/DPT002.xhtml" login-required="true">
		<action execute="#{departmentManagementFormBean.init()}"
			if="#{departmentManagementFormBean.init and departmentManagementFormBean.updateParam == null}" />
		<action execute="#{departmentManagementFormBean.updateInit()}"
			if="#{departmentManagementFormBean.init and departmentManagementFormBean.updateParam != null}" />
		<navigation>
			<rule if-outcome="init">
				<redirect view-id="/view/DPT002.xhtml" />
			</rule>
			<rule if-outcome="error">
				<redirect view-id="/view/DPT002.xhtml" />
			</rule>
			<rule if-outcome="ok">
				<redirect view-id="/view/DPT002.xhtml" />
			</rule>
			<rule if-outcome="search">
				<redirect view-id="/view/DPT001.xhtml" />
			</rule>
			<rule if-outcome="back">
				<redirect view-id="/view/DPT001.xhtml" />
			</rule>
		</navigation>
